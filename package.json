{
  "name": "azure-webapp-route-traffic",
  "version": "1.0.0",
  "private": true,
  "description": "Route traffic to a slot on an Azure Web App",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "yarn run build && yarn run format && yarn run lint && yarn run pack && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https:/https://github.com/colindembovsky/azure-webapp-route-traffic.git"
  },
  "keywords": [
    "web app",
    "slot",
    "traffic",
    "route"
  ],
  "author": "Colin Dembovsky",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "azure-actions-webclient": "^1.0.11"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "nock": "^12.0.3",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
